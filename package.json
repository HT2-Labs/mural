{
  "name": "@ht2-labs/mural",
  "version": "1.0.0",
  "description": "Demonstrates ht2-labs with mural.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ht2-labs/mural"
  },
  "main": "dist/index.js",
  "author": "ht2-labs",
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc",
    "dev": "npm run clean && parcel src/index.html",
    "lint": "tslint --project ./tsconfig.json",
    "duplication": "jscpd",
    "clean": "rimraf dist .cache",
    "semantic-release": "ht2-release-public-circleci-lib-dist",
    "build-ghpages": "npm run clean && parcel build src/index.html --public-url ./",
    "deploy-ghpages": "node scripts/ghpages.js",
    "ghpages": "npm run build-ghpages && npm run deploy-ghpages"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.0",
    "styled-components": "^3.3.0"
  },
  "devDependencies": {
    "@ht2-labs/semantic-release": "1.1.6",
    "@ht2-labs/typescript-project": "1.0.24",
    "@types/prop-types": "15.5.4",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "@types/react-router-dom": "4.3.0",
    "gh-pages": "1.2.0",
    "jscpd": "0.6.22",
    "parcel-bundler": "1.9.7",
    "react-router-dom": "4.3.1",
    "react-syntax-highlighter": "8.0.1",
    "rimraf": "2.6.2",
    "typescript": "3.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
